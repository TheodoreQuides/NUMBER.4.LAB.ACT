def teddy(score):
    if score < 0:
        print("Invalid Grade! Please enter a value between 60 and 100.")
         
    elif score <= 60:
        print("GRADE F")        
    elif score <= 65:
        print("GRADE D")        
    elif score <= 75:
        print(" GRADE C")        
    elif score <= 90:
        print("GRADE B")        
    elif score <= 100:
        print(" GRADE A")
    else: 
        print("bagsak")
       
print("Assigning Grades")

while True:
    try:
        gonzales = input("State you Grade: ")
        teddy(int(gonzales))        
    except ValueError:
        print("Invalid score! Please enter a value between 0 and 100.")
        break
